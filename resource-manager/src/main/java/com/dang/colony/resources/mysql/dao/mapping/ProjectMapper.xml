<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dang.colony.resources.mysql.dao.ProjectMapper" >
  <resultMap id="BaseResultMap" type="com.dang.colony.resources.mysql.model.Project" >
    <id column="id" property="id"  />
    <result column="name" property="name"  />
  </resultMap>
